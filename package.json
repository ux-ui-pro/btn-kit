{
  "name": "btn-kit",
  "version": "0.1.4",
  "description": "btn-kit is a Vue 3 component for creating buttons for web applications. Built-in features include icon support, different button types and sizes, states, effects.",
  "author": "ux-ui.pro",
  "homepage": "https://btn-kit.ux-ui.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ux-ui-pro/btn-kit.git"
  },
  "bugs": {
    "url": "https://github.com/ux-ui-pro/btn-kit/issues"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/btn-kit.umd.cjs",
  "module": "./dist/btn-kit.js",
  "exports": {
    ".": {
      "import": "./dist/btn-kit.js",
      "require": "./dist/btn-kit.umd.cjs"
    }
  },
  "exclude": [
    "src/App.vue",
    "src/main.js"
  ],
  "scripts": {
    "build": "vite build",
    "lint:js": "eslint --ext .js,.vue .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.3.4",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.3",
    "sass": "^1.68.0"
  },
  "keywords": [
    "vue",
    "button",
    "ripple",
    "component",
    "custom",
    "shape",
    "rounded",
    "round",
    "sizes",
    "buttons",
    "variation",
    "href",
    "disabled",
    "router",
    "vuejs",
    "click",
    "progress",
    "contained",
    "outlined",
    "text",
    "theme"
  ]
}
